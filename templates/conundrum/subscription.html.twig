{% extends 'base.html.twig' %}

{% block title %}{{ 'conundrum.subscription.head_title'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'conundrum.subscription.title'|trans }}</h1>
    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        {{ form_row(form.submit) }}
    {{ form_end(form) }}
{% endblock %}
